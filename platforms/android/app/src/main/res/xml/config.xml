<?xml version='1.0' encoding='utf-8'?>
<widget id="com.planing24.mobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="ContactsPhoneNumbers">
        <param name="android-package" value="com.dbaq.cordova.contactsPhoneNumbers.ContactsManager" />
    </feature>
    <name>Planing 24</name>
    <description>
        Самый простой планировщик для малого бизнеса
    </description>
    <author email="support@flajok.ru" href="http://planing24.com">
	Planing24.com
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://netlify.ru" />
    <preference name="loglevel" value="DEBUG" />
</widget>
